  "description": "Tool to automate checking for updates and downloading newer versions for NW.js apps",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.cjs --config=.eslintrc.cjs index.cjs src tests nw-testing",
